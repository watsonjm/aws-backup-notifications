# Bash
terraform init -var-file=./config/temp.tfvars -reconfigure
terraform plan -var-file=./config/temp.tfvars -out temp.tfplan
terraform apply temp.tfplan

# PowerShell
terraform init -var-file /config/temp.tfvars -reconfigure
terraform plan -var-file /config/temp.tfvars -out temp.tfplan
terraform apply temp.tfplan